<% var title = 'Authentication Example' %>
<% include head %>

<h1>Register</h1>
<form method="post" action="/register">
  <p>
    <label>Username:</label>
    <input type="text" name="username">
  </p>
  <p>
    <label>Password:</label>
    <input type="text" name="password">
  </p>
  <p>
    <input type="submit" value="Login">
  </p>
</form>

<% include foot %>
